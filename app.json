{
  "expo": {
    "name": "CropTrends",
    "slug": "croptrends",
    "scheme": "croptrends",
    "description": "A Crop pricing App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/croptrends_logo.png",
    "userInterfaceStyle": "light",
