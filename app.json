        "foregroundImage": "./assets/croptrends_logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.tamil_devs.croptrends"
    },
    "web": {
      "favicon": "./assets/croptrends_logo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cefd275c-abc7-4a8c-a141-f2bd4ce54a5f"
      }
    },
    "owner": "tamil_devs"
